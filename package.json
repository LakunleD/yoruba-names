{
    "name": "yoruba-names",
    "version": "0.0.0-development",
    "description": "Get random yoruba names",
    "main": "src/index.js",
    "scripts": {
        "commit": "git-cz",
        "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
        "report-coverage": "cat coverage/lcov.info | codecov",
        "test": "mocha tests/index.test.js -w",
        "test:single": "istanbul cover -x tests/*.js _mocha -- -R spec tests/index.test.js",
        "semantic-release": "semantic-release pre && npm publish && semantic-release post"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mczlatan/yoruba-names.git"
    },
    "keywords": [
        "random",
        "yoruba",
        "names"
    ],
    "author": "mczlatan <bournethesonofthegun@gmail.com> (https://mczlatan.github.io/)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mczlatan/yoruba-names/issues"
    },
    "homepage": "https://github.com/mczlatan/yoruba-names#readme",
    "dependencies": {
        "ghooks": "2.0.0",
        "istanbul": "0.4.5",
        "unique-random-array": "1.0.0"
    },
    "devDependencies": {
        "chai": "4.1.2",
        "codecov.io": "0.1.6",
        "commitizen": "^2.9.6",
        "cz-conventional-changelog": "^2.0.0",
        "mocha": "4.0.1",
        "semantic-release": "^8.0.3"
    },
    "czConfig": {
        "path": "node_modules/cz-conventional-changelog"
    },
    "config": {
        "ghooks": {
            "pre-commit": "npm run test:single && npm run check-coverage"
        }
    }
}